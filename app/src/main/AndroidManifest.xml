<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tf.NewsDemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.tf.NewsDemo.activity.FlashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tf.NewsDemo.activity.MainActivity" />
        <activity android:name="com.tf.NewsDemo.activity.NewsActivity" />
        <activity android:name="com.tf.NewsDemo.activity.CommentsActivity" />
        <activity android:name="com.tf.NewsDemo.activity.ThemeActivity" />
        <activity android:name="com.tf.NewsDemo.activity.LoginActivity"/>
        <activity android:name="com.tf.NewsDemo.activity.EditorActivity">
        </activity>
    </application>

</manifest>
